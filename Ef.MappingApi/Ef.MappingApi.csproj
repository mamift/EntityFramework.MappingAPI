<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <RootNamespace>EntityFramework.MappingAPI</RootNamespace>
    <Version>0.5.0</Version>
    <Authors>github.com/mamift</Authors>
    <Description>A port of https://archive.codeplex.com/?p=efmappingapi to .NET Framework 4.5.2. Forked from https://github.com/OpenCST/EntityFramework.MappingAPI. Specifically built to remove the signed assembly.</Description>
    <Copyright>Original authors on archive.codeplex.com/?p=efmappingapi and github.com/OpenCST/EntityFramework.MappingAPI.</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/mamift/EntityFramework.MappingAPI</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mamift/EntityFramework.MappingAPI</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE;EF6</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EntityFramework" Version="6.1.3" />
  </ItemGroup>

</Project>
